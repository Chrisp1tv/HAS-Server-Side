        <body>
            <!-- General -->
            <!-- Words and simple expressions -->
            <trans-unit id="and">
                <source>and</source>
                <target>et</target>
            </trans-unit>
            <trans-unit id="alertSystem">
                <source>alertSystem</source>
                <target>Système d'alertes</target>
            </trans-unit>
            <trans-unit id="homepage">
                <source>homepage</source>
                <target>Accueil</target>
            </trans-unit>
            <trans-unit id="administrators">
                <source>administrators</source>
                <target>Administrateurs</target>
            </trans-unit>
            <trans-unit id="campaigns">
                <source>campaigns</source>
                <target>Campagnes de messages</target>
            </trans-unit>
            <trans-unit id="recipients">
                <source>recipients</source>
                <target>Destinataires</target>
            </trans-unit>
            <trans-unit id="recipientGroups">
                <source>recipientGroups</source>
                <target>Groupes de destinataires</target>
            </trans-unit>
            <trans-unit id="realizedBy">
                <source>realizedBy</source>
                <target>Réalisation :</target>
            </trans-unit>
            <trans-unit id="duplicateCampaign">
                <source>duplicateCampaign</source>
                <target>Dupliquer une campagne</target>
            </trans-unit>
            <trans-unit id="newRecipientGroup">
                <source>newRecipientGroup</source>
                <target>Nouveau groupe de destinataires</target>
            </trans-unit>
            <trans-unit id="modifyRecipientGroup">
                <source>modifyRecipientGroup</source>
                <target>Modifier le groupe %groupName%</target>
            </trans-unit>
            <trans-unit id="login">
                <source>login</source>
                <target>Connexion</target>
            </trans-unit>
            <trans-unit id="username">
                <source>username</source>
                <target>Nom d'utilisateur</target>
            </trans-unit>
            <trans-unit id="password">
                <source>password</source>
                <target>Mot de passe</target>
            </trans-unit>
            <!-- Sentences -->
            <trans-unit id="sentence.numberOfCampaigns">
                <source>sentence.numberOfCampaigns</source>
                <target>
                    {0}Aucune campagne n'est enregistrée.
                    |{1}Une campagne de message est enreigstée.
                    |] 1, Inf]%count% campagnes de messages sont enregistrées.
                </target>
            </trans-unit>
            <trans-unit id="sentence.numberOfUnsentCampaigns">
                <source>sentence.numberOfUnsentCampaigns</source>
                <target>
                    {0}
                    |{1}Une campagne doit encore être envoyée.
                    |]1, Inf[%count% doivent encore être envoyées.
                </target>
            </trans-unit>
            <trans-unit id="sentence.lastConnectionDate">
                <source>sentence.lastConnectionDate</source>
                <target>La dernière connexion à votre compte administrateur a eu lieu le %connectionDate%, via l'IP
                    %IPConnection%
                </target>
            </trans-unit>
            <trans-unit id="sentence.numberOfLinkedRecipients">
                <source>sentence.numberOfLinkedRecipients</source>
                <target>
                    {0}Aucun ordinateur n'est lié au système d'alertes.
                    |{1}Un ordinateur est lié au système d'alertes.
                    |]1, Inf[%count% ordinateurs sont liés au système d'alertes.
                </target>
            </trans-unit>
            <!-- Forms -->
            <!-- Forms labels -->
            <trans-unit id="form.label.save">
                <source>form.label.save</source>
                <target>Sauvegarder</target>
            </trans-unit>
            <trans-unit id="form.label.name">
                <source>form.label.name</source>
                <target>Nom</target>
            </trans-unit>
            <trans-unit id="form.label.content">
                <source>form.label.content</source>
                <target>Contenu</target>
            </trans-unit>
            <trans-unit id="form.label.color">
                <source>form.label.color</source>
                <target>Couleur (code hexadécimal)</target>
            </trans-unit>
            <trans-unit id="form.label.bold">
                <source>form.label.bold</source>
                <target>Gras</target>
            </trans-unit>
            <trans-unit id="form.label.message">
                <source>form.label.message</source>
                <target>Message</target>
            </trans-unit>
            <trans-unit id="form.label.sendingDate">
                <source>form.label.sendingDate</source>
                <target>Date et heure d'envoi</target>
            </trans-unit>
            <trans-unit id="form.label.endDate">
                <source>form.label.endDate</source>
                <target>Date d'expiration</target>
            </trans-unit>
            <trans-unit id="form.label.repetitionFrequency">
                <source>form.label.repetitionFrequency</source>
                <target>Fréquence de répétition (en minutes)</target>
            </trans-unit>
            <trans-unit id="form.label.recipients">
                <source>form.label.recipients</source>
                <target>Destinataires</target>
            </trans-unit>
            <trans-unit id="form.label.recipientGroups">
                <source>form.label.recipientGroups</source>
                <target>Groupes de destinataires</target>
            </trans-unit>
            <trans-unit id="form.label.linkingIdentifier">
                <source>form.label.linkingIdentifier</source>
                <target>Identifiant de liaison</target>
            </trans-unit>
            <!-- Form placeholders -->
            <trans-unit id="form.placeholder.repetitionFrequency">
                <source>form.placeholder.repetitionFrequency</source>
                <target>Laissez vide si aucune répétition</target>
            </trans-unit>
            <!-- Validation -->
            <!-- Administrator validation -->
            <trans-unit id="validation.administrator.username.notBlank">
                <source>validation.administrator.username.notBlank</source>
                <target>Veuillez renseigner le nom d'utilisateur de la campagne.</target>
            </trans-unit>
            <trans-unit id="validation.administrator.username.length.min">
                <source>validation.administrator.username.length.min</source>
                <target>Le nom d'utilisateur doit posséder au moins {{ limit }} caractères.</target>
            </trans-unit>
            <trans-unit id="validation.administrator.username.length.max">
                <source>validation.administrator.username.length.max</source>
                <target>Le nom d'utilisateur ne peut posséder plus de {{ limit }} caractères.</target>
            </trans-unit>
            <trans-unit id="validation.administrator.password.notBlank">
                <source>validation.administrator.password.notBlank</source>
                <target>Veuillez renseigner le mot de passe de l'administrateur.</target>
            </trans-unit>
            <trans-unit id="validation.administrator.password.length.min">
                <source>validation.administrator.password.length.min</source>
                <target>Le mot de passe doit posséder au moins {{ limit }} caractères.</target>
            </trans-unit>
            <trans-unit id="validation.administrator.password.length.max">
                <source>validation.administrator.password.length.max</source>
                <target>Le mot de passe ne peut posséder plus de {{ limit }} caractères.</target>
            </trans-unit>
            <!-- Campaign validation -->
            <trans-unit id="validation.campaign.expression">
                <source>validation.campaign.expression</source>
                <target>La campagne doit obligatoirement posséder des destinataires (individuels ou groupés).</target>
            </trans-unit>
            <trans-unit id="validation.campaign.name.notBlank">
                <source>validation.campaign.name.notBlank</source>
                <target>La campagne doit obligatoirement posséder un nom.</target>
            </trans-unit>
            <trans-unit id="validation.campaign.name.length.min">
                <source>validation.campaign.name.length.min</source>
                <target>Le nom de la campagne doit posséder au moins {{ limit }} caractères.</target>
            </trans-unit>
            <trans-unit id="validation.campaign.name.length.max">
                <source>validation.campaign.name.length.max</source>
                <target>Le nom de la campagne ne peut posséder plus de {{ limit }} caractères.</target>
            </trans-unit>
            <trans-unit id="validation.campaign.sender.notNull">
                <source>validation.campaign.sender.notNull</source>
                <target>La campagne doit posséder un émetteur.</target>
            </trans-unit>
            <trans-unit id="validation.campaign.message.notNull">
                <source>validation.campaign.message.notNull</source>
                <target>La campagne doit posséder un message.</target>
            </trans-unit>
            <trans-unit id="validation.campaign.sendingDate.date">
                <source>validation.campaign.sendingDate.date</source>
                <target>La date d'envoi saisie n'est pas valide.</target>
            </trans-unit>
            <trans-unit id="validation.campaign.sendingDate.greaterThan">
                <source>validation.campaign.sendingDate.greaterThan</source>
                <target>Il n'est pas possible d'envoyer une campagne dans le passé.</target>
            </trans-unit>
            <trans-unit id="validation.campaign.endDate.date">
                <source>validation.campaign.endDate.date</source>
                <target>La date de fin saisie n'est pas valide.</target>
            </trans-unit>
            <trans-unit id="validation.campaign.endDate.expression">
                <source>validation.campaign.endDate.expression</source>
                <target>La campagne ne peut se terminer avant d'avoir commencé.</target>
            </trans-unit>
            <trans-unit id="validation.campaign.repetitionFrequency.range.min">
                <source>validation.campaign.repetitionFrequency.range.min</source>
                <target>La fréquence minimale de répétition ne peut être inférieure à {{ limit }} minutes.</target>
            </trans-unit>
            <!-- Message validation -->
            <trans-unit id="validation.message.content.notBlank">
                <source>validation.message.content.notBlank</source>
                <target>Votre message doit impérativement posséder un contenu.</target>
            </trans-unit>
            <trans-unit id="validation.message.color.regex">
                <source>validation.message.color.regex</source>
                <target>La couleur saisie n'a pas été reconnue.</target>
            </trans-unit>
            <!-- RecipientGroup validation -->
            <trans-unit id="validation.recipientGroup.name.notBlank">
                <source>validation.recipientGroup.name.notBlank</source>
                <target>Votre groupe doit impérativement posséder un nom.</target>
            </trans-unit>
            <trans-unit id="validation.recipientGroup.name.length.min">
                <source>validation.recipientGroup.name.length.min</source>
                <target>Le nom de votre groupe doit posséder au moins {{ limit }} caractères.</target>
            </trans-unit>
            <trans-unit id="validation.recipientGroup.name.length.max">
                <source>validation.recipientGroup.name.length.max</source>
                <target>Le nom de votre groupe ne peut posséder plus de {{ limit }} caractères.</target>
            </trans-unit>
            <trans-unit id="validation.recipientGroup.recipients.min">
                <source>validation.recipientGroup.recipients.min</source>
                <target>Votre groupe doit posséder au moins {{ limit }} destinataires.</target>
            </trans-unit>
            <!-- Flash messages -->
            <trans-unit id="flash.campaignDuplicated">
                <source>flash.campaignDuplicated</source>
                <target>La campagne a bien été dupliquée en tenant compte des informations fournies.</target>
            </trans-unit>
            <trans-unit id="flash.recipientGroupCreated">
                <source>flash.recipientGroupCreated</source>
                <target>Le groupe de destinataires a bien été créé.</target>
            </trans-unit>
            <trans-unit id="flash.recipientGroupModified">
                <source>flash.recipientGroupModified</source>
                <target>Le groupe de destinataires a bien été modifié.</target>
            </trans-unit>
            <trans-unit id="flash.administratorCreated">
                <source>flash.administratorCreated</source>
                <target>L'administrateur a bien été créé.</target>
            </trans-unit>
        </body>
