{% extends 'form_div_layout.html.twig' %}

{%- block form_errors -%}
    {%- if errors|length > 0 -%}
        {%- for error in errors -%}
            {% include "common/modules/flash-message.html.twig" with {type:'error', content:error.message} only %}
        {%- endfor -%}
    {%- endif -%}
{%- endblock form_errors -%}
